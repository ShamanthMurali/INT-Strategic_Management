        <mxCell id="ertDeUdLPHvkuPLK6w1d-23" value="Marketing &#xa;Techniques " style="swimlane;startSize=40;" vertex="1" collapsed="1" parent="1">
          <mxGeometry x="-5.409999999999997" y="1110" width="120" height="40" as="geometry">
